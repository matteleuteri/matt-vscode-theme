{
	"name": "matt-vscode-theme",
	"colors": {
		"editor.background": "#260546",
		"editor.foreground": "#ffffff"
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#4444ff"
			}
		},

		{
			"scope": "comment",
			"settings": {
				"foreground": "#555555"
			}
		},

		{
			"scope": ["source.cpp", 
				"constant",
				"keyword.control.if",
				"keyword.control.else",
			   	"storage.modifier.pointer",
				"punctuation.separator.delimiter",
				"punctuation.terminator.statement.",
				"keyword.control"],
			"settings" : {
				"foreground" : "#ff0000"
			}
		},
